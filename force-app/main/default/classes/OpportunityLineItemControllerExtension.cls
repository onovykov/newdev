public class OpportunityLineItemControllerExtension {

	private final OpportunityLineItem oppProduct;

	public OpportunityLineItemControllerExtension(ApexPages.StandardController stdController) {
		this.oppProduct = (OpportunityLineItem) stdController.getRecord();
	}

	public PageReference saveRecord() {		
		String soql = ApexUtil.getCreatableFieldsSOQL('OpportunityLineItem', oppProduct.Id);
		OpportunityLineItem opl = (OpportunityLineItem) Database.query(soql);
		OpportunityLineItem oppLI = opl.clone(false, true);
		oppLI.ID_ERP__c = null;		
		oppLI.TotalPrice = null;
		insert oppLI;

		PageReference nextPage = new PageReference('/' + oppLI.Id);
		return nextPage;
	}

