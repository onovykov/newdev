    // Конструктор
    public AccountMatches(String MainAccountId, String MainAcc_ID_ERP, String MainAccName,
                        String AccountId, String AccountName, String AccountID_ERP, Decimal CosineSimilarity,
                        /*Integer CountCommonWords, */ Decimal JaccardSimilarity, Integer LevenshteinDistance,
                        Decimal MatchFirstWordsCoef, Decimal NamesCorrelation, Decimal NormalizedLevenshtein,
                        String SoundexMainAccount, String SoundexDuplicate, /*Integer SoundexMatchCount, Decimal SoundexMatchIndex,*/
                        Decimal SoundexSimilarity, DateTime CreatedDate,
                        String ActivityType, String ParentId, String HoldingGroup, String BillingCountryCode, String ShippingCountryCode,
                        String MarketSegment, String Type, String BillingCountry, String BillingCity, 
                        String ShippingCountry, String ShippingCity, String OwnerId, String CreatedById, Boolean UnderSanctions) {
        this.MainAccountId = MainAccountId;
        this.MainAcc_ID_ERP = MainAcc_ID_ERP;
        this.MainAccName = MainAccName;
        this.AccountId = AccountId;
        this.AccountName = AccountName;
        this.AccountID_ERP = AccountID_ERP;
        this.CosineSimilarity = CosineSimilarity;
        /*this.CountCommonWords = CountCommonWords;*/
        this.JaccardSimilarity = JaccardSimilarity;
        this.LevenshteinDistance = LevenshteinDistance;
        this.MatchFirstWordsCoef = MatchFirstWordsCoef;
        this.NamesCorrelation = NamesCorrelation;
        this.NormalizedLevenshtein = NormalizedLevenshtein;
        this.SoundexMainAccount = SoundexMainAccount;
        this.SoundexDuplicate = SoundexDuplicate;
        //this.SoundexMatchCount = SoundexMatchCount;
        //this.SoundexMatchIndex = SoundexMatchIndex;
        this.SoundexSimilarity = SoundexSimilarity;
        this.CreatedDate = CreatedDate;
        this.ActivityType = ActivityType;
        this.ParentId = ParentId;
        this.HoldingGroup = HoldingGroup;
        this.BillingCountryCode = BillingCountryCode;
        this.ShippingCountryCode = ShippingCountryCode;
        this.MarketSegment = MarketSegment;
        this.Type = Type;
        this.BillingCountry = BillingCountry;
        this.BillingCity = BillingCity;
        this.ShippingCountry = ShippingCountry;
        this.ShippingCity = ShippingCity; 
        this.OwnerId = OwnerId; 
        this.CreatedById = CreatedById;
        this.UnderSanctions = UnderSanctions;
    }

