global class ProductionQuoteGroupAllBatch implements Database.Batchable<AggregateResult>, Database.Stateful {
	
    global List<Date> listDateCheck = new List<Date>();
    global List<id> insertedProductionQuote = new List<Id>();
    
	global ProductionQuoteGroupAllBatch(){
        system.debug('start ProductionQuoteGroupAllBatch');   
        Map<String,Deleted_Period__c> records = Deleted_Period__c.getAll();
        //List<Date> listDateCheck = new List<Date>();
        system.debug(records);
        for(String dp : records.keySet()){
            listDateCheck.add(date.valueOf(dp));
        }
        
        system.debug(listDateCheck);
        
    }    
    
    global Iterable<AggregateResult> start(Database.BatchableContext context) {		
        system.debug('in start ProductionQuoteGroupAllBatch');
        
        return  new AggregateResultIterable('SELECT sum(Production_Amount_t__c),Market_Segment__c,Period__c,Product__c,Quota_Type__c,Sales_Manager__c,Shop__c '+  
                                        'FROM Production_Quota__c WHERE Period__c in:listDateCheck '+
                                        'group by Market_Segment__c,Period__c,Product__c,Quota_Type__c,Sales_Manager__c,Shop__c' ,listDateCheck);
	}
    
    global void execute(Database.BatchableContext context, List<sObject> scope) {
        List<Production_Quota__c> prodQuotaToInsert = new List<Production_Quota__c>();
        for(sObject sc: scope){
            Production_Quota__c newProductionQuote = new Production_Quota__c();
            newProductionQuote.Market_Segment__c = String.valueOf(sc.get('Market_Segment__c'));
            newProductionQuote.Period__c = Date.valueOf(sc.get('Period__c'));
            newProductionQuote.Product__c = String.valueOf(sc.get('Product__c'));
            newProductionQuote.Quota_Type__c = String.valueOf(sc.get('Quota_Type__c'));
            newProductionQuote.Sales_Manager__c = String.valueOf(sc.get('Sales_Manager__c'));  
            newProductionQuote.Shop__c  = String.valueOf(sc.get('Shop__c'));
            newProductionQuote.Production_Amount_t__c = Double.valueOf(sc.get('expr0')); 
            prodQuotaToInsert.add(newProductionQuote);
        }
        insert prodQuotaToInsert;
        
        for(Production_Quota__c pq:prodQuotaToInsert){
            insertedProductionQuote.add(pq.Id);
        }
        system.debug('created prod quotes');
        
                             
	}
    
    global void finish(Database.BatchableContext context) {
        List<Production_Quota__c> pqList = [SELECT id FROM Production_Quota__c WHERE Period__c in:listDateCheck and Id not in:insertedProductionQuote];
        delete pqList;
        
        // createBatchProdQuote = new ProductionQuoteAddRelativeItems(insertedProductionQuote);
		//Id createBatchProdQuoteId = Database.executeBatch(createBatchProdQuote,30);
        //System.scheduleBatch( createBatchProdQuote, 'test3', 4,30);
	}

