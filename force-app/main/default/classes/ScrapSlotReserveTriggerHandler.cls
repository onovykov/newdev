    public void setFieldsValues (List<ScrapSlotReserve__c> newRecords) {
        for(ScrapSlotReserve__c rec : newRecords) {
            Decade__c decadeRec;
            String sDate = DTUtils.formatDate(rec.ReserveDate__c, 'yyyy-MM-dd');
            try {
                String strSQL = 'SELECT Id, Parent__c, Parent__r.Parent__c, Parent__r.Parent__r.Parent__c ' +
                                'FROM Decade__c ' +
                                'WHERE Date__c = ' + sDate + ' LIMIT 1';
                decadeRec = Database.query(strSQL);
            } catch (QueryException e) {
                decadeRec = null;
            }
            if(decadeRec!=null) {
                rec.DateLookup__c = decadeRec.Id;
                rec.DecadeLookup__c = decadeRec.Parent__c;
                rec.MonthLookup__c = decadeRec.Parent__r.Parent__c;
                rec.YearLookup__c = decadeRec.Parent__r.Parent__r.Parent__c;
            }
        }
    }

