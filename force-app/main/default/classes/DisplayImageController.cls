    @AuraEnabled(cacheable=true)
    public static List<String> getResourceURL(String resourceName, String resourceNameKLW) {
        String businessDivision = UserUtils.getBusinessDivision();
        System.debug('resourceName : ' + resourceName);
        System.debug('resourceNameKLW : ' + resourceNameKLW);
        if(businessDivision == 'Railway' && String.isNotBlank(resourceNameKLW)) {
            resourceName = resourceNameKLW;
        }
        System.debug('resourceName after : ' + resourceName);
        List<String> resourceNames = (List<String>)JSON.deserialize(resourceName, List<String>.class);
            List<StaticResource> resourceList = [
                SELECT Name, NamespacePrefix, SystemModStamp 
                FROM StaticResource 
                WHERE Name in: resourceNames
        ];
            List<String> resources = new List<String>();                 
            for (StaticResource res : resourceList) {
                
                String namespace = res.NamespacePrefix;
                resources.add('/NewCustomerRoom/s/sfsites/c/resource/' 
                + res.SystemModStamp.getTime() + '/' 
                + (namespace != null && namespace != '' ? namespace + '__' : '') 
                + res.Name);
            }
           System.debug('RESOURCES ' + resources);
           return resources;
    }

